{
  "name": "Carro",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": [
        "find",
        "findById",
        "create",
        "prototype.patchAttributes",
        "deleteById"
      ]
    }
  },
  "properties": {
    "marca": {
      "type": "string",
      "required": true
    },
    "modelo": {
      "type": "string",
      "required": true
    },
    "placa": {
      "type": "string",
      "required": true
    },
    "cor": {
      "type": "string",
      "required": true
    },
    "proprietario": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
